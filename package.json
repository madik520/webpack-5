{
  "name": "webpack-5",
  "version": "1.0.0",
  "description": "webpack-5-template",
  "main": "index.js",
  "scripts": {
    "start": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" NODE_ENV=development webpack serve --mode development --progress",
    "build": "rimraf build/* && cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" NODE_ENV=production API_DEV_ENDPOINT=$API_ENDPOINT webpack --progress --config ./webpack.config.ts",
    "build:development": "rimraf build/* && cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" NODE_ENV=production API_DEV_ENDPOINT=$API_ENDPOINT webpack --progress --config ./webpack.config.ts",
    "clean:local": "rimraf dist \"node_modules/!(rimraf|.bin)\" package-lock.json",
    "clean:global": "npx --ignore-existing rimraf dist node_modules package-lock.json",
    "reinstall:local-clean": "npm run clean:local && npm install",
    "reinstall:global-clean": "npm run clean:global && npm install",
    "format:eslint": "eslint --fix src/**/*.{ts,tsx} ./**.{ts,js}",
    "format:stylelint": "stylelint src/**/*.scss",
    "format:prettier": "prettier --write src/**/*.{ts,tsx} ./**.{ts,js}",
    "format:commit": "lint-staged",
    "rel": "standard-version",
    "prepare": "husky install",
    "prepare:folders": "make-dir src/utils src/pages src/hooks src/hoc src/components src/assets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madik520/webpack-5.git"
  },
  "keywords": [
    "webpack5",
    "webpack-5",
    "webpack-template"
  ],
  "author": "madik520",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/madik520/webpack-5/issues"
  },
  "homepage": "https://github.com/madik520/webpack-5#readme",
